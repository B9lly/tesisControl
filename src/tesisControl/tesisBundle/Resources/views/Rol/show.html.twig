{% extends 'tesisControltesisBundle::Template.html.twig' %}
{% block body -%}
    <h1>Rol</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombrer</th>
                <td>{{ entity.nombreR }}</td>
            </tr>
            <tr>
                <th>Descripcionr</th>
                <td>{{ entity.descripcionR }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('rol') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('rol_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
