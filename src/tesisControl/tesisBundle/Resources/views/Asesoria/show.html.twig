{% extends 'tesisControltesisBundle::Template.html.twig' %}

{% block body -%}
    <h1>Asesoria</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Horainicio</th>
                <td>{{ entity.horaInicio | date("H:i:s") }}</td>
            </tr>
            <tr>
                <th>Horafin</th>
                <td>{{ entity.horaFin | date("H:i:s")}}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('asesoria') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('asesoria_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}

