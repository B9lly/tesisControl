{% extends 'tesisControltesisBundle::Template.html.twig' %}

{% block body -%}
    <h1>Perfil</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>
                <th>Entidad</th>
                <td>{{ entity.entidad }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('perfil') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('perfil_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
